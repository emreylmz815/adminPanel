@model WebApplication1.Models.CRM.ViewModels.PaymentPlanFormViewModel
@{
    ViewBag.Title = "Edit Payment Plan";
}
<h2>@ViewBag.Title</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.Partial("_PlanForm", Model)
    <button type="submit" class="btn btn-primary">Save</button>
    @Html.ActionLink("Cancel", "Plans", null, new { @class = "btn btn-default" })
}
